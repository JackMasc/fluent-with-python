   "source": [
    "# Basic operations with rows and columns in a numpy.ndarray\n",
    "\n",
    "import numpy\n",
    "a = numpy.arange(12)\n",
    "print(a)\n",
    "print(type(a))\n",
    "print(a.shape)\n",
    "a.shape = 3,4\n",
    "print(a)\n",
    "print(a[2])\n",
    "print(a[2,1])\n",
    "print(a[:,1])\n",
    "a.transpose()"
   ]
